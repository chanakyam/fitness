<body>
<div class="headerbg">
  <div class="container">
    <header> <a href="/" title="I Have Fitness"><img src="/images/logo.png" alt="" /></a>
      <span class="clear"></span>
      <nav>
        <ul>
          <li><a href="/" title="Home">Home</a></li>
          <li class="active"><a href="/beauty" title="Beauty">Beauty</a></li>
          <li><a href="/videos?c=by_id_title_desc_thumb_date&p=1" title="Video">Videos</a></li>     
        </ul>
      </nav>
      <div class="topAd">
        <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=728X90&cwpid=557677&cwwidth=728&cwheight=90&cwpnet=1&cwtagid=205120"></script>
      </div>
      <span class="clear"></span> </header>
    <section class="hotnews"> <span id="blink" class="blinking_text">{{ beautynews1.title }} </span></section>
    <section class="contentarea">
    <div class="module video-stream">
      <div class="video-title"><h2>{{videoParam.title}} <span>{{videoParam.upload_date}}</span></h2> </div>
      <input type="hidden" name="video_name" id="video_name" value="{{videoParam.video}}" />
      <input type="hidden" name="video_tit" id="video_tit" value="{{videoParam.title}}">
      <input type="hidden" name="video_desc" id="video_desc" value="{{videoParam.description}}">
      <input type="hidden" name="video_duration" id="video_duration" value="{{videoParam.duration}}">
      <div id="myElement">Loading the player...</div>
              
    </div><!-- video module end -->
      <aside class="leftcontent">
      
      
      {% for n in beautynews %}
        {% if forloop.counter==1 or forloop.counter==3 or forloop.counter==5 %}
          <div class="newsrow">
        {% endif %}
        {% if forloop.counter==5%}
        <div class="newsrow">
        <div class="ad-photo-gal">
            <div class="ad1">
              <div class="rightad">
                <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=300X250&cwpid=557677&cwwidth=300&cwheight=250&cwpnet=1&cwtagid=205119"></script>
              </div>
            </div>
            <div class="PhotoGal"> 
            {% for n in fitnessimgs %}             
              <a  href="/news/{{ n.id }}" title="{{ n.title }}">
                <img  src="http://newscdn.lycos.com/{{ n.thumb_image }}" alt="" >
              </a>
            {% endfor %}              
            </div>
          </div>
          <div class="gap"></div>
        {% endif %}
          <div class="LeftNewsDiv">
            <a href="beautynews/{{ n.id }}"><img src="http://newscdn.lycos.com/{{ n.thumb_image }}" alt="" title="{{n.title}}"/></a>
            <div class="description">
              <h3><a href="beautynews/{{ n.id }}">{{ n.title }} </a></h3>
              <p class="padding10_TB"><a href="beautynews/{{ n.id }}">{{ n.description }} </a></p>
              <p class="date"><span>{{ n.upload_date }}</span></p>
            </div>
          </div>
          {% if forloop.counter==1 or forloop.counter==3 or forloop.counter==5 %}
          <div class="gap"></div>
          {% endif %}
          {% if forloop.counter==2 or forloop.counter==4 or forloop.counter==5 %}
          </div >
        {% endif %}
          {% endfor %}      
        
      </aside>
      <aside class="rightcontent">
      	<div class="RightNewsDiv">
            <a href="beautynews/{{ beautynews8.id }}">
              <img src="http://newscdn.lycos.com/{{ beautynews8.thumb_image }}" alt="" title="{{ beautynews8.title }}"/>
            </a>
            <div class="description">
              <h3><a href="beautynews/{{ beautynews8.id }}">{{ beautynews8.title }}</a></h3>
              <p class="padding10_TB"><a href="beautynews/{{ beautynews8.id }}">{{ beautynews8.description }} </a></p>
              <p class="date"><span>{{ beautynews8.upload_date }}</span></p>
            </div>
            <!-- description end --> 
          </div>
          <!-- RightNewsDiv end -->
          <div class="rightad margin7_T">
            <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=300X250&cwpid=557677&cwwidth=300&cwheight=250&cwpnet=1&cwtagid=205119"></script>
          </div> 
          <!-- <div class="RightNewsDiv">
            <a href="beautynews/{{ beautynews9.id }}">
              <img src="http://newscdn.lycos.com/{{ beautynews9.thumb_image }}" alt="" title="{{ beautynews9.title }}"/>
            </a>
            <div class="description">
              <h3><a href="beautynews/{{ beautynews9.id }}">{{ beautynews9.title }}</a></h3>
              <p class="padding10_TB"><a href="beautynews/{{ beautynews9.id }}">{{ beautynews9.description }} </a></p>
              <p class="date"><span>{{ beautynews9.upload_date }}</span></p>
            </div>
          </div> -->
          <!-- RightNewsDiv end -->     
        <div class="twitterAdd">
          <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=160X600&cwpid=557677&cwwidth=160&cwheight=600&cwpnet=1&cwtagid=205118"></script>
        </div>
        <!-- </div> --><!-- twittertweets -->
      </aside>
    <span class="clear"></span> 
    <a href="/morebeautynews?c=beauty_view&p=1" class="showmore">
      <span>Show more</span>
    </a> 
  </section>
    <span class="clear"></span> </div>
</div>
<!-- headerbg end -->
<div class="bottomad">
  <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=728X90&cwpid=557677&cwwidth=728&cwheight=90&cwpnet=1&cwtagid=205120"></script>
